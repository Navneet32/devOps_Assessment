pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
              echo 'Your job build is completed.'
            }
        }
        stage('Test') {
          steps {
              echo 'Your jobs testing is completed.'
            }
        }
        stage('Deploy') {
          steps {
              echo 'your job has been deployed.'
            }
        }
    }
}
