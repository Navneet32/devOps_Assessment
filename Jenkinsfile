pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
              echo 'Your job build is completed.'
            }
        }
        stage('Test') {
          steps {
              echo 'Your job's testing is completed.'
            }
        }
        stage('Deploy'){
           steps{
              echo 'Your job has been deployed.'
           }
        }
    }
}
